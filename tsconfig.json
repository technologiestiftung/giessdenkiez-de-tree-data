{
	"compilerOptions": {
		"allowImportingTsExtensions": true,
		"noEmit": true,
		"target": "ESNext",
		"module": "NodeNext",
		"rootDir": "./src",
		"moduleResolution": "NodeNext",
		"erasableSyntaxOnly": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	},
	"exclude": ["data-wrangling"]
}
